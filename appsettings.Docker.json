{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "QdrantSettings": {
    "Endpoint": "http://qdrant:6333",
    "CollectionName": "knowledge",
    "VectorSize": 768
  },
  "LMStudioSettings": {
    "Endpoint": "http://host.docker.internal:1234",
    "EmbeddingModel": "text-embedding-nomic-embed-text-v2-moe",
    "ChatModel": "qwen/qwen2.5-coder-32b-instruct",
    "Temperature": 0.7,
    "MaxTokens": 2048
  },
  "DocumentProcessing": {
    "DataPath": "./data",
    "ChunkSize": 500,
    "ChunkOverlap": 50,
    "SupportedExtensions": [
      ".txt",
      ".pdf"
    ]
  }
}